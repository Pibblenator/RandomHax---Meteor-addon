{
  "schemaVersion": 1,
  "id": "randomhax",
  "version": "${version}",
  "name": "RandomHax",
  "description": "A Meteor Client addon with random (but powerful) hacks.",
  "authors": ["ZyZaBruh"],
  "contact": {
    "sources": "https://github.com/Pibblenator/RandomHax---Meteor-addon"
  },
  "icon": "assets/randomhax/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.randomhax.addon.RandomHax"
    ]
  },
  "mixins": [
    "randomhax.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.8", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
